{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node",
      "config": {
        "prisma": {
          "generate": "prisma generate"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "src/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ]
}
